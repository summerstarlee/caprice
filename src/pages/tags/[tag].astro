---
import Layout from '../../layouts/Layout.astro';

export async function getStaticPaths() {
    return [
        { params: { tag: "astro" } },
        { params: { tag: "successes" } },
        { params: { tag: "community" } },
        { params: { tag: "blogging" } },
        { params: { tag: "setbacks" } },
        { params: { tag: "learning in public" } },
    ];
}

const { tag } = Astro.params;
---

<Layout>
    <p>Posts tagged with {tag}</p>
</Layout>



